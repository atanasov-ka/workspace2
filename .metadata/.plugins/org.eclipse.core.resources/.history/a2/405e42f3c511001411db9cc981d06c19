package crawer;

import java.io.IOException;
import java.net.URL;

public class Main {

	public static void main(String[] args) {
		try {
			System.out.println(Crawer.search(new URL("http://ebusiness.free.bg"), "Револвираща", 10, "UTF-8"));
		} catch (IOException e) {
			e.printStackTrace();
		}
	}
}
